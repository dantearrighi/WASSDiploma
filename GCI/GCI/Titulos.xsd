                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Profesionales.dni, Profesionales.nombre_apellido, Profesionales.fecha_nacimiento, Profesionales.sexo, Profesionales.telefono, Profesionales.celular, 
                         Profesionales.email1, Profesionales.email2, Direcciones.direccion, Localidades.nombre AS Localidad, Provincias.nombre AS Provincia, 
                         Tipos_Documentos.descripcion AS Tipo_doc, Tipos_Matriculas.descripcion AS Tipo_mat, Matriculas.icie AS mat, Planes.año AS [Plan], Titulos.descripcion AS Título, 
                         Universidades.descripcion AS Universidad, Especialidades.descripcion AS Especialidad, Jurisdicciones.descripcion AS Jurisdicción, 
                         Modalidades.descripcion AS Modalidad, Niveles.descripcion AS Nivel, Direcciones.id
FROM            Localidades INNER JOIN
                         Provincias ON Localidades.Provincia_id = Provincias.id INNER JOIN
                         Profesionales INNER JOIN
                         Direcciones ON Profesionales.dni = Direcciones.Profesional_dni INNER JOIN
                         Tipos_Documentos ON Profesionales.Tipo_Documento_id = Tipos_Documentos.id ON Localidades.id = Direcciones.Localidad_id INNER JOIN
                         Tipos_Matriculas ON Profesionales.Tipo_Matricula_id = Tipos_Matriculas.id INNER JOIN
                         Matriculas ON Profesionales.dni = Matriculas.Profesional_dni INNER JOIN
                         Legajos_Academicos ON Matriculas.Legajo_Academico_id = Legajos_Academicos.id INNER JOIN
                         Planes ON Legajos_Academicos.Plan_id = Planes.id INNER JOIN
                         Titulos ON Legajos_Academicos.Titulo_id = Titulos.id INNER JOIN
                         Universidades ON Titulos.Universidad_id = Universidades.id INNER JOIN
                         Especialidades ON Titulos.Especialidad_id = Especialidades.id INNER JOIN
                         Jurisdicciones ON Titulos.Jurisdiccion_id = Jurisdicciones.id INNER JOIN
                         Modalidades ON Titulos.Modalidad_id = Modalidades.id INNER JOIN
                         Niveles ON Titulos.Nivel_id = Niveles.id
WHERE        (Profesionales.dni = @dni) AND (Direcciones.id = @id_dir) AND (Matriculas.icie = @icie)</CommandText>
