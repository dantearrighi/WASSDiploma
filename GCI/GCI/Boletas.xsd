                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Profesionales.nombre_apellido AS NyA, Movimientos.importe, Movimientos.descripcion AS Cuota, Localidades.cp AS CP, Localidades.nombre AS Localidad, 
                         Provincias.nombre AS Provincia, Direcciones.direccion
FROM            Cuotas INNER JOIN
                         Profesionales ON Cuotas.Profesional_dni = Profesionales.dni INNER JOIN
                         Direcciones ON Profesionales.dni = Direcciones.Profesional_dni INNER JOIN
                         Localidades ON Direcciones.Localidad_id = Localidades.id INNER JOIN
                         Provincias ON Localidades.Provincia_id = Provincias.id INNER JOIN
                         Movimientos ON Cuotas.descripcion = Movimientos.descripcion
WHERE        (Cuotas.estado = 0) AND (Profesionales.Tipo_Matricula_id = @tipo_matricula) AND (Cuotas.id &gt;= @menor) AND (Cuotas.id &lt;= @mayor) AND 
                         (Movimientos.descripcion LIKE '%' + @desc + '%')
GROUP BY Profesionales.nombre_apellido, Movimientos.importe, Movimientos.descripcion, Localidades.cp, Localidades.nombre, Provincias.nombre, Direcciones.direccion</CommandText>
