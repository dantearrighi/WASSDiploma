          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="MovimientosTableAdapter" GeneratorDataComponentClassName="MovimientosTableAdapter" Name="Movimientos" UserDataComponentName="MovimientosTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="GCIConnectionString2 (Settings)" DbObjectName="GCI.dbo.Especialidades" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        SUM(Movimientos.importe) AS Cantidad, Especialidades.descripcion
FROM            Movimientos INNER JOIN
                         CtasCtes ON Movimientos.CtaCte_id = CtasCtes.id INNER JOIN
                         Profesionales ON CtasCtes.Profesional_dni = Profesionales.dni INNER JOIN
                         Matriculas ON Profesionales.dni = Matriculas.Profesional_dni INNER JOIN
                         Legajos_Academicos ON Matriculas.Legajo_Academico_id = Legajos_Academicos.id INNER JOIN
                         Titulos ON Legajos_Academicos.Titulo_id = Titulos.id INNER JOIN
                         Especialidades ON Titulos.Especialidad_id = Especialidades.id
WHERE        (Movimientos.descripcion LIKE '%' + 'expediente' + '%') AND (Movimientos.Tipo_Movimiento_id = 2) AND (Matriculas.id =
                             (SELECT        MIN(id) AS Expr1
                               FROM            Matriculas AS Matriculas_1
                               WHERE        (Profesional_dni = Matriculas.Profesional_dni))) AND (Movimientos.fecha &gt;= @fecha_menor) AND (Movimientos.fecha &lt;= @fecha_mayor)
GROUP BY Especialidades.descripcion</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="true" AutogeneratedName="fecha_menor" ColumnName="fecha" DataSourceName="GCI.dbo.Movimientos" DataTypeServer="datetime2" DbType="AnsiString" Direction="Input" ParameterName="@fecha_menor" Precision="0" ProviderType="DateTime2" Scale="0" Size="8" SourceColumn="fecha" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="true" AutogeneratedName="fecha_mayor" ColumnName="fecha" DataSourceName="GCI.dbo.Movimientos" DataTypeServer="datetime2" DbType="AnsiString" Direction="Input" ParameterName="@fecha_mayor" Precision="0" ProviderType="DateTime2" Scale="0" Size="8" SourceColumn="fecha" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
