                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Profesionales.nombre_apellido, Direcciones.direccion, Localidades.nombre, Localidades.cp, Matriculas.icie, Titulos.descripcion
FROM            Profesionales INNER JOIN
                         Direcciones ON Profesionales.dni = Direcciones.Profesional_dni INNER JOIN
                         Localidades ON Direcciones.Localidad_id = Localidades.id INNER JOIN
                         Matriculas ON Profesionales.dni = Matriculas.Profesional_dni INNER JOIN
                         Legajos_Academicos ON Matriculas.Legajo_Academico_id = Legajos_Academicos.id INNER JOIN
                         Titulos ON Legajos_Academicos.Titulo_id = Titulos.id
WHERE        (Matriculas.icie = @icie)
GROUP BY Profesionales.nombre_apellido, Direcciones.direccion, Localidades.nombre, Localidades.cp, Matriculas.icie, Titulos.descripcion</CommandText>
